    "def entrada_dados():\n",
    "    dados = []\n",
    "    i = 0\n",
    "    while i < 4:\n",
    "        dados.append(int(input(f\"Digite o {i+1}º número.\")))\n",
    "        i+=1\n",
    "    return dados"
